{
    "name": "appventus/mangopay-bundle",
    "type": "symfony-bundle",
    "description": "This Bundle include Mangopay php sdk as a service",
    "keywords": ["mangopay"],
    "homepage": "http://appventus.com",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Paul Andrieux",
            "homepage": "https://appventus.com"
        }
    ],
    "require": {
        "php"                       : ">=5.0.0",
        "symfony/framework-bundle"  : ">=2.3",
        "mangopay/php-sdk-v2"       : ">=2.0",
        "appventus/ajax-bundle"     : "*"
    },

    "autoload": {
        "psr-0": { "AppVentus\\MangopayBundle": "" }
    },

    "target-dir": "AppVentus/MangopayBundle"
}
